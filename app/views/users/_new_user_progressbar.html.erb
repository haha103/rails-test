<div class="progress">
	<% 1.upto(@user_create_phases.length) do |i| %>
		<div class="progress-bar <%= current_phase < i ? "progress-bar-gray" : (current_phase > i ? "progress-bar-success" : "progress-bar-warning") %>" role="progressbar" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100" style="width: 25%;">
			<span><%= "#{i}. #{@user_create_phases[i - 1]}" %></span>
		</div>
	<% end %>
</div>
