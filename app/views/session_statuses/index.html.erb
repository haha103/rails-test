<h1>Listing session_statuses</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @session_statuses.each do |session_status| %>
      <tr>
        <td><%= session_status.code %></td>
        <td><%= session_status.name %></td>
        <td><%= link_to 'Show', session_status %></td>
        <td><%= link_to 'Edit', edit_session_status_path(session_status) %></td>
        <td><%= link_to 'Destroy', session_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Session status', new_session_status_path %>
